color1 = #4d4d45
color2 = #bababa

*
	color #ffffff
	font-family "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif
	font-size 11px
	letter-spacing 1px
	text-decoration none
	-webkit-text-size-adjust none
	outline none !important

body
	color #ffffff
	font-family "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif
	font-size 13px
	letter-spacing 1px
	text-decoration none
	overflow hidden
	background url(images/bg_transparencia.png)

	form#fContacto
		input
			display block
		
		input[type="submit"], input[type="reset"]
			display inline-block

a, h3
	color inherit

#wrap
	min-height 100%

	.arrow
		cursor pointer
		top 50%
		width 35px
		position absolute

	.reg
		// @extend .arrow;
		left 20px


	.sig
		// @extends .arrow;
		right 20px

header
	height 100px
	// background url(images/deg_top.png) repeat-x 0px 0px
	-webkit-background-size auto 100%
	-moz-background-size auto 100%
	background-size auto 100%
	z-index 9010

	nav
		display none
		position absolute
		top 20px
		width 100%	
		z-index 100
		
		> ul 
			> li:first-child
				padding-left 50px
		ul
			height 20px
			list-style-type none
			margin 0px
			padding 0px 5px 5px 0px
			position relative
			width 100%	 

			li:after
				content " |"
				display inline
			
			li
				background url("images/bg_transparencia.png")
				cursor pointer
				cursor hand
				float left
				padding 5px

				h3
					display: inline;
			
				ul
					display none
					margin-left 0px
					position absolute !important
					top 26px
				
				li 
					ul
						position absolute !important
						top 26px

			li:last-of-type:after
				content ""

			li:hover > ul
				display block

			li

				h3:hover, a:hover 
					color color2;



#salas:first-letter, #sucursales:first-letter, .quienesSomos:first-letter
	text-transform capitalize

h3:first-letter
	text-transform capitalize


a#logo
	width 250px
	height 0px
	background color1
	position absolute
	top -50px
	right 160px
	overflow hidden

img#logo-t
	width 250px
	height auto
	margin-top 110px

li ul
	text-align left
	margin-top 10px\9
	margin-left -80px\9

div#estilos, div#modelos, div#sucursal
	padding 15px
	width 940px
	height 440px

	.contenedor


	h1
		text-transform uppercase
		font-weight bold
		font-size 18px		

	article
		width 30%
		padding 1.6%
		display inline-block

		img
			width 200px
			height 150px
			margin 10px

		img:hover
			border 1px solid white
			margin 9px

		h2
			font-size 16px
			text-transform uppercase
			padding-left 10px

div#thumb
	display inline-block
	height 410px
	overflow hidden
	position absolute
	top 15px
	width 140px

	h3
		margin-left 10px
		font-size 10px
		margin-bottom 3px
		text-transform uppercase

	.contenedor

		.jspContainer
			width 130px

			.jspPane
				padding 0px !important

div.contenedor
	position relative
	top 0px
	height 400px

p#mas
	position absolute
	top 420px
	left 45px
	cursor hand
	cursor pointer

p#regresar
	padding-left 30px
	background url(../images/flecha_reg_w.png) 0px 50% no-repeat
	background-size auto 12px
	cursor hand
	cursor pointer
	position absolute
	top 445px
	left 25px
	z-index 900

div#vinetas
	display inline-block
	height 420px
	margin 0px 0px 0px 20px
	overflow hidden
	position absolute
	top 15px
	left 715px
	width 200px

	hgroup h1#nombreModelo
		font-size 18px
		text-transform uppercase
		font-weight bold

	hgroup h2.estilo, div#estilos article div h2
		font-size 12px
		text-transform uppercase
	
	div#estilos article
		cursor pointer
		cursor hand
	img
		margin 0 auto

	img#vineta
		margin 20px auto 0 auto

	span
		font-size 0.85em
		width 200px
		position absolute
		bottom 60px
		left 0px

#ft
	bottom 30px
	position absolute

#ft img
	margin-right 10px
	width 20px

img.tmb
	width 100px
	height 75px
	margin 0px 10px 10px 10px

#principal
	position absolute
	top 15px
	left 155px
	overflow hidden

#principal img
	width 560px
	height 420px

#principal div#frase
	position absolute
	bottom 20px
	left 0px
	width 45%
	background url(images/bg_transparencia.png)
	padding 10px 10px
	padding-right 5px
	font-size 11px

footer
	font-size 9px
	text-align right
	width 100%
	z-index 10
	bottom 0px
	position fixed
	letter-spacing 1px

footer div
	background url(images/bg_transparencia.png)
	padding 10px
	text-align right
	width 80em
	z-index 10
	bottom 0px
	margin 0 auto

footer img
	margin -5px 5px 0px 5px
	float left
	width 20px
	height 20px

#fondo1, #fondo2, #bg_sucursal, #bg_quienesSomos
	width 100%
	height 100%
	z-index -2
	position absolute
	top 0px
	left 0px
	padding 0px
	background-size 100% auto

#fondo1 img, #fondo2 img
	width 100%
	position absolute

#bg_sucursal, #bg_quienesSomos
	z-index -1
	display none
	background url(images/bg_transparencia.png) no-repeat
	-webkit-background-size 100% auto
	-moz-background-size 100% auto
	background-size 100% auto

.nivo-main-image
	background-size 100%
	background-position 0 0% !important
	position relative
	top -100%

.bg_img
	display none
	position absolute
	top 0
	left 0

ul
	border none

.ui-menu
	list-style none
	padding 0px
	margin 0
	display block
	width 120px
	border none

.ui-menu .ui-menu
	margin-top 0px
	border none

.ui-menu .ui-menu-item
	margin 0
	padding 0
	border none

.ui-menu .ui-menu-item a
	text-decoration none
	display block
	padding 0
	border none

.ui-menu .ui-menu-item a.ui-state-hover,
.ui-menu .ui-menu-item a.ui-state-active
	margin 0px
	border none

.ui-slider a
	outline none !important

div#sucursal
	// background url(css/images/bg_transparencia_0.7.png)
	position absolute
	height 350px
	width 950px

	div
		position relative
		width 320px
		height 150px
		top 0px
		left 30px
		overflow hidden

		h1
			top 60px
			left 40px
			width 100%
			margin-bottom 10px
			font-size 22px
			text-transform uppercase

		p
			top 100px
			left 40px
			font-size 12px

			span
				display block
				margin-bottom 0.5em

	img#map
		width 340px
		height auto
		position absolute
		top 130px
		left 30px

	img#fachada
		width 500px
		height auto
		position absolute
		top 30px
		left 400px

div#texto
	background url(images/bg_transparencia.png)
	overflow hidden
	padding 15px
	position absolute

	h1
		color inherit
		font-size 20px
		margin-bottom 5%

	p
		color inherit
		font-size 13px
		margin-bottom 5%

#mainCell
	background url(images/bg_transparencia.png)

.jspContainer
	overflow hidden
	position relative

.jspPane
	position absolute

.jspPane p
	margin-left 10px

.jspVerticalBar
	background-image url(images/bg_transparencia_0.4.png)
	height 100%
	position absolute
	right 0
	top 0
	width 8px

div#estilos
	h1
		font-weight normal

	.jspVerticalBar
		right 50px

.jspHorizontalBar
	background red
	bottom 0
	height 16px
	left 0
	position absolute
	width 100%

.jspVerticalBar *,
.jspHorizontalBar *
	margin 0
	padding 10

.jspCap
	display none

.jspHorizontalBar .jspCap
	float left

.jspTrack
	background rgba(186, 186, 186, 0.4)
	position relative

.jspDrag
	-moz-border-radius 5px
	-webkit-border-radius 5px
	background color2
	border-radius 5px
	border 1px solid color1
	cursor pointer
	left 0
	position relative
	top 0

.jspHorizontalBar .jspTrack,
.jspHorizontalBar .jspDrag
	float left
	height 100%
	width 6px

.jspArrow
	background color1
	cursor pointer
	display block
	text-indent -20000px

.jspArrow.jspDisabled
	background #80808d
	cursor default

.jspVerticalBar .jspArrow
	height 16px

.jspHorizontalBar .jspArrow
	float left
	height 100%
	width 6px

.jspVerticalBar .jspArrow focus
	outline none

.jspCorner
	background #eeeef4
	float left
	height 100%

* html .jspCorner
	margin 0 -3px 0 0
